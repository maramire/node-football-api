{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [1, "ignorePackages"],
        "no-underscore-dangle": "off",
        "no-console": 0
    },
    "ignorePatterns": ["**/*.pem"]
}
